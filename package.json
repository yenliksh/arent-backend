{
  "name": "livin",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.10.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"database/seeds/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\" -c .eslintrc.ci.js",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cp-env": "cp .env dist",
    "d-build": "docker build -t nestjs-example .",
    "d-run": "docker run --rm -it -p 8000:5000 nestjs-example",
    "dc-up": "docker-compose up",
    "dc-up:build": "docker-compose up --build",
    "dc-down": "docker-compose down",
    "commit": "git-cz",
    "_smart-knex": "node -r tsconfig-paths/register -r ts-node/register node_modules/.bin/knex",
    "migration:make": "yarn knex migrate:make -- -x ts",
    "migration:up": "yarn _smart-knex migrate:up",
    "migration:latest": "yarn _smart-knex migrate:latest",
    "migration:rollback": "yarn _smart-knex migrate:rollback",
    "migration:down": "yarn _smart-knex migrate:down",
    "migration:list": "yarn _smart-knex migrate:list",
    "seed:make": "yarn _smart-knex seed:make -- -x ts",
    "seed:run": "yarn _smart-knex seed:run",
    "staging:migration:latest": "yarn _smart-knex --env staging migrate:latest",
    "staging:seed:run": "yarn _smart-knex --env staging seed:run",
    "production:migration:latest": "yarn _smart-knex --env production migrate:latest",
    "production:seed:run": "yarn _smart-knex --env production seed:run",
    "typeorm-migration:make": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm-migration:latest": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "typeorm-migration:rollback": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "prepare": "husky install"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.13.0",
    "@faker-js/faker": "^7.3.0",
    "@fastify/formbody": "6.0.0",
    "@fastify/static": "^6.4.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/cqrs": "^8.0.5",
    "@nestjs/elasticsearch": "^8.1.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-fastify": "^8.4.7",
    "@nestjs/swagger": "^6.0.3",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@sentry/node": "^7.10.0",
    "@sentry/tracing": "^7.10.0",
    "@ssut/nestjs-sqs": "^1.3.0",
    "apollo-server-fastify": "^3.8.2",
    "aws-cloudfront-sign": "^2.2.0",
    "aws-elasticsearch-connector": "9.0.3",
    "aws-sdk": "^2.1152.0",
    "bull": "^4.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.0.0",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "fastify-swagger": "^5.2.0",
    "geo-tz": "^7.0.2",
    "googleapis": "^126.0.0",
    "graphql": "^16.5.0",
    "graphql-redis-subscriptions": "2.4.2",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.2.2",
    "joi": "^17.6.0",
    "knex": "^2.1.0",
    "libphonenumber-js": "^1.10.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "nestjs-i18n": "^9.1.1",
    "newrelic": "^9.0.0",
    "objection": "^3.0.1",
    "oxide.ts": "^1.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.13",
    "@types/faker": "4.1.0",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog-for-jira-smart-commits": "^1.0.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "28.0.3",
    "lint-staged": "^13.0.2",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "semantic-release": "^19.0.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "validate-branch-name": "^1.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@libs/(.*)": "<rootDir>/libs/$1",
      "@modules/(.*)": "<rootDir>/modules/$1",
      "@infrastructure/(.*)": "<rootDir>/infrastructure/$1",
      "@domains/(.*)": "<rootDir>/rental-context/domains/$1",
      "@domain-value-objects/(.*)": "<rootDir>/rental-context/domain-value-objects/$1",
      "@third-parties/(.*)": "<rootDir>/third-parties/$1",
      "src/(.*)": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-for-jira-smart-commits"
    }
  },
  "validate-branch-name": {
    "pattern": "^ARENT-\\d{1,5}-(feature|fix|chore|hotfix|refactor)/.+$|^(feature|fix|chore|hotfix|refactor)/.+$",
    "errorMsg": "Invalid branch name"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator"
    ]
  }
}
